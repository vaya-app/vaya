    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.2.0",
    "vite": "^2.0.1"
  },
  "engines": {
    "node": "14.2.0",
    "npm": "6.14.4"
  }
}